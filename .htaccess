# Disable directory browsing
Options -Indexes

RewriteEngine On
RewriteBase /khaservice-it/

# Redirect root access to public/
RewriteRule ^$ public/ [L]

# Redirect all requests not starting with public/ to public/ directory
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^((?!public/).*)$ public/$1 [L,QSA]